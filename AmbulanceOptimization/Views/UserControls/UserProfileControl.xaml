<UserControl x:Class="AmbulanceOptimization.Views.UserControls.UserProfileControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AmbulanceOptimization.Views.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"  >
    <Grid>
        <StackPanel Width="100" Height="150" VerticalAlignment="Top" Orientation="Vertical" Margin="0,25,100,0" HorizontalAlignment="Right">
            <Button HorizontalAlignment="left" VerticalAlignment="Top" Width="40" Height="40" Click="ProfileButton_Click" Margin="0,0,0,5">
                <Image Source="/Views/Images/ProfileButton.png"  Stretch="Uniform"/>
            </Button>
            <StackPanel Orientation="Vertical" Name="UserDropDown">
               
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
